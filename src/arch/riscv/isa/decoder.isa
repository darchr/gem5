                                                                    ExceptionCode::LOAD_ADDR_MISALIGNED);
                            }

                            fault =
                                readMemAtomicLE(xc,
                                                traceData,
                                                target_address,
                                                Mem_ub,
                                                memAccessFlags);
                            if (fault != NoFault) {
                                return fault;
                            }
                            Vd_vc[regElemID] = Mem_ub;
                        }});
                    }
                    0x10: VectorUnitStrideMemLoadOp::vle8ff_v({{
                        uint64_t target_address = Rs1_ud + offset;
                        if (target_address % eewb) {
                            // Alignment fault.
                            return std::make_shared<AddressFault>(target_address,
                                                                ExceptionCode::LOAD_ADDR_MISALIGNED);
                        }

                        fault =
                            readMemAtomicLE(xc,
                                            traceData,
                                            target_address,
                                            Mem_ub,
                                            memAccessFlags);
                        if (fault != NoFault) {
                            return fault;
                        }
                        Vd_vc[regElemID] = Mem_ub;
                    }});
                }
                0x1: VectorIndexedMemLoadOp::vluxei8_v({{
                    uint64_t offset = 0;
                    switch (eewb) {
                        case 1:
                            offset = Vs2_container.as<uint8_t>()[srcVecRegElemID];
                            break;
                        case 2:
                            offset = Vs2_container.as<uint16_t>()[srcVecRegElemID];
                            break;
                        case 4:
                            offset = Vs2_container.as<uint32_t>()[srcVecRegElemID];
                            break;
                        case 8:
                            offset = Vs2_container.as<uint64_t>()[srcVecRegElemID];
                            break;
                    }

                    uint64_t target_address = Rs1_ud + offset;
                    if (target_address % sewb) {
                        // Alignment fault.
                        return std::make_shared<AddressFault>(target_address,
                                                              ExceptionCode::LOAD_ADDR_MISALIGNED);
                    }

